{
    "name": "claritytech/cms",
    "type": "library",
    "description": "content management system package",
    "keywords": [
        "claritytech",
        "cms"
    ],
    "homepage": "https://github.com/claritytech/cms",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Brown",
            "email": "andy@claritytech.com",
            "homepage": "http://claritytech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",
        "illuminate/support": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "claritytech\\cms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "claritytech\\cms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
